<?xml version="1.0" encoding="utf-8"?>

<robot
  name="diff_robots" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:arg name="simulated_robot" default="diffRobot"/>
  <xacro:property name="simulated_robot_pkg" value="$(arg simulated_robot)" />
  <xacro:include filename="$(find ${simulated_robot_pkg})/urdf/$(arg simulated_robot).urdf.xacro"/>
  <xacro:include filename="$(find simulation_env)/urdf/robot_io.gazebo"/>


</robot>